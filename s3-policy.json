{
  "Version":"2012-10-17",
  "Statement":[
    {
      "Effect":"Allow",
      "Action":["s3:ListBucket"],
      "Resource":["arn:aws:s3:::aws-image-processor-uploads"]
    },
    {
      "Effect":"Allow",
      "Action":["s3:GetObject"],
      "Resource":["arn:aws:s3:::aws-image-processor-uploads/*"]
    },
    {
      "Effect":"Allow",
      "Action":["s3:PutObject"],
      "Resource":["arn:aws:s3:::aws-image-processor-processed/*"]
    }
  ]
}
